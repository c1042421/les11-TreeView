<Window x:Class="oefTreeView.frmInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:oefTreeView"
        mc:Ignorable="d"
        Title="frmInput" Height="133.613" Width="300">
    <DockPanel>
        <TextBox Name="txtAntwoord" DockPanel.Dock="Bottom" Margin="5"></TextBox>
        <StackPanel DockPanel.Dock="Right">
            <Button Name="btnOk" Margin="5" Click="btnOk_Click">OK</Button>
            <Button Name="btnCancel" Margin="5" Click="btnCancel_Click">Cancel</Button>
        </StackPanel>
        <Label Name="lblVraag"></Label>
        
    </DockPanel>
</Window>
